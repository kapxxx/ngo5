                                    <!-- Main-menu -->
                                    <div class="main-menu d-none d-lg-block">
                                        <nav>
                                            <ul id="navigation">
                                                @* <li><a href="index.html">Home</a></li>*@
                                                <li>@Html.ActionLink("Home", "Index", "Home")</li>
                                                <li>@Html.ActionLink("About", "About", "Home")</li>
                                                <li>@Html.ActionLink("Donations", "Donation", "Home")</li>
                                                <li>@Html.ActionLink("Blogs", "Blogs", "Home")</li>
                                                <li>@Html.ActionLink("Raise Fund", "RaiseFund", "Home")</li>
                                                
                                                @* <li><a href="@Html.ActionLink("Home","Contact")">Contact</a></li>*@
                                                <li>@Html.ActionLink("Contact", "Contact", "Home")</li>

                                                @{
                                                    if (Session["Username"] == null)
                                                    {
                                                        <li>@Html.ActionLink("Login", "Login", "Home")</li>
                                                      
                                                    }
                                                    else
                                                    {
                                                        <li>Welcome @Session["Username"] </li>

                                                        <li>
                                                            <a href="#">Profile</a>
                                                            <ul class="submenu">
                                                                <li>@Html.ActionLink("My Donation funds", "MydonatonFund", "Home")</li>
                                                              @*  <li><a href="blog_details.html">Ma Raised Fund</a></li>*@
                                                                <li>@Html.ActionLink("Logout", "Logout", "Home")</li>

                                                            </ul>
                                                        </li>
                                                    }
